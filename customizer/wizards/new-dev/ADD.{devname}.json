{
  "firmware" : {
    "mcu_model" : "stm32f103",
    "device_name" : "{devname}_tester",
    "sys_tick": {
      "requires" : {"timer": "TIM4"}},
    "i2c_bus" : {
      "clock_speed" : 100000,
      "buffer_size" : 512,
      "address" : 19,
      "requires" :  {"i2c" : "I2C2"}
    }},

  "devices" : {
    "{DevName}Customizer": {
      "{devname}_0" : {
        "dev_id" : 1,
        "buffer_size" : 64,
        "requires" :  {}
      },

      "{devname}_1" : {
        "dev_id" : 2,
        "buffer_size" : 64,
        "requires" :  {}
      }
    }
  }
}