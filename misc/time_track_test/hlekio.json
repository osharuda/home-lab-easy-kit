{
	"board": "Raspberry Pi Zero",
	"distro": "Raspberry Pi OS",
	"inputs": {
		"ttdev_warn" : {
			"pin"     : "GPIO_8",
			"pull"    : "up",
			"trigger" : "high"
		},
		"ttdev_ok" : {
			"pin"     : "GPIO_7",
			"pull"    : "up",
			"trigger" : "low"
		}
	},
	"outputs": {
		"ttdev_idle"    : {
			"pin"     : "GPIO_10",
			"pin-type": "push-pull",
			"default" : "high"
		},
		"ttdev_progress"  : {
			"pin"     : "GPIO_9",
			"pin-type": "push-pull",
			"default" : "low"
		},
		"ttdev_error"    : {
			"pin"     : "GPIO_11",
			"pin-type": "push-pull",
			"default" : "low"
		}
	}
}
