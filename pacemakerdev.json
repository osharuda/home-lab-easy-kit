          "internal_timer": { "timer" : "TIM3"},
          "signal_0"   : {"gpio" :  "PB_1"},
          "signal_1"   : {"gpio" :  "PB_2"},
          "signal_2"   : {"gpio" :  "PB_3"},
          "signal_3"   : {"gpio" :  "PB_4"}
        }
      }
    }
  }
